<h2>Manage</h2>

<ul class="albums">
<% for album in @albums %>
	<li>
		<a href="/gallery/<%= album.id %>">
			<div><img src="/image/<%= album_photo album.id %>/large" alt="<%= album.name %>"></div>
			<p><%= album.name %></p>
    </a>
    <a href="/build/manage/delete/<%= album.id %>" class="delete">delete</a>
	</li>
<% end %>
</ul>
